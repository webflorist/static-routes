{
    "name": "webflorist/static-routes",
    "description": "Generate static/hybrid pages for Laravel-Routes",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
        "laravel/framework": ">=5.5 <10.0.0"
	},
    "require-dev": {
        "orchestra/testbench": ">=3.5"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\StaticRoutes\\": "src/",
            "StaticRoutesTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\StaticRoutes\\StaticRoutesServiceProvider"
            ]
        }
    }
}
